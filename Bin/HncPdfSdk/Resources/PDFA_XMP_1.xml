    <rdf:RDF xmlns:rdf = "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
        <rdf:Description
            rdf:about = ""
            xmlns:pdfaExtension = "http://www.aiim.org/pdfa/ns/extension/"
            xmlns:pdfaSchema = "http://www.aiim.org/pdfa/ns/schema#"
            xmlns:pdfaProperty = "http://www.aiim.org/pdfa/ns/property#"
            xmlns:pdfaType = "http://www.aiim.org/pdfa/ns/type#"
            xmlns:pdfaField = "http://www.aiim.org/pdfa/ns/field#"
            xmlns:pdfaValidate = "http://www.pdf-d.org/pdfa/ns/validate/">
            <pdfaExtension:schemas>
                <rdf:Bag>
                    <!--Extention schema used for vatidation of XMP schemas -->
                    <rdf:li>
                        <rdf:Description pdfaValidate:predefined = "False" >
                            <pdfaSchema:schema>PDF/A XMP validation schema</pdfaSchema:schema>
                            <pdfaSchema:namespaceURI>http://www.pdf-d.org/pdfa/ns/validate/</pdfaSchema:namespaceURI>
                            <pdfaSchema:prefix>pdfaValidate</pdfaSchema:prefix>
                            <pdfaSchema:property>
                                <rdf:Seq>
                                     <rdf:li>
                                        <rdf:Description>
                                            <pdfaProperty:name>predefined</pdfaProperty:name>
                                            <pdfaProperty:valueType>Boolean</pdfaProperty:valueType>
                                            <pdfaProperty:category>internal</pdfaProperty:category>
                                            <pdfaProperty:description>Predefined or unpredefined whole schema or certain property or type. Default is True (predefined)</pdfaProperty:description>
                                        </rdf:Description>
                                    </rdf:li>
                                   <rdf:li>
                                        <rdf:Description pdfaValidate:constraint = "^required|prohibited|restricted|deprecated|recommended|ignored$">
                                            <pdfaProperty:name>status</pdfaProperty:name>
                                            <pdfaProperty:valueType>closed Choice</pdfaProperty:valueType>
                                            <pdfaProperty:category>internal</pdfaProperty:category>
                                            <pdfaProperty:description>Normative status: required, prohibited, restricted, deprecated, recommended, ignored</pdfaProperty:description>
                                        </rdf:Description>
                                    </rdf:li>
                                    <rdf:li>
                                        <rdf:Description>
                                            <pdfaProperty:name>default</pdfaProperty:name>
                                            <pdfaProperty:valueType>Text</pdfaProperty:valueType>
                                            <pdfaProperty:category>internal</pdfaProperty:category>
                                            <pdfaProperty:description>A default value for required property. It shall be corresponding type of the property value</pdfaProperty:description>
                                        </rdf:Description>
                                    </rdf:li>
                                   <rdf:li>
                                        <rdf:Description>
                                            <pdfaProperty:name>constraint</pdfaProperty:name>
                                            <pdfaProperty:valueType>Text</pdfaProperty:valueType>
                                            <pdfaProperty:category>internal</pdfaProperty:category>
                                            <pdfaProperty:description>Regular expression used to constrain property values</pdfaProperty:description>
                                        </rdf:Description>
                                    </rdf:li>
                                    <rdf:li>
                                        <rdf:Description>
                                            <pdfaProperty:name>subst</pdfaProperty:name>
                                            <pdfaProperty:valueType>Text</pdfaProperty:valueType>
                                            <pdfaProperty:category>internal</pdfaProperty:category>
                                            <pdfaProperty:description>Property Name of a predefined schema property used to for substitution instead this one</pdfaProperty:description>
                                        </rdf:Description>
                                    </rdf:li>
                                    <rdf:li>
                                        <rdf:Description pdfaValidate:status = "restricted" pdfaValidate:constraint = "^pdf|pdfa|pdfd|xmp$">
                                            <pdfaProperty:name>standard</pdfaProperty:name>
                                            <pdfaProperty:valueType>closed Choice</pdfaProperty:valueType>
                                            <pdfaProperty:category>internal</pdfaProperty:category>
                                            <pdfaProperty:description>Name of the compliance standard for the error message: pdf or pdfa</pdfaProperty:description>
                                        </rdf:Description>
                                    </rdf:li>
                                    <rdf:li>
                                        <rdf:Description>
                                            <pdfaProperty:name>count</pdfaProperty:name>
                                            <pdfaProperty:valueType>Text</pdfaProperty:valueType>
                                            <pdfaProperty:category>internal</pdfaProperty:category>
                                            <pdfaProperty:description>Max count of items for array value type properties (e.g. dc:creator) </pdfaProperty:description>
                                        </rdf:Description>
                                    </rdf:li>
                                     <rdf:li>
                                        <rdf:Description>
                                            <pdfaProperty:name>clause</pdfaProperty:name>
                                            <pdfaProperty:valueType>Text</pdfaProperty:valueType>
                                            <pdfaProperty:category>internal</pdfaProperty:category>
                                            <pdfaProperty:description>Clause of the error message from the specification</pdfaProperty:description>
                                        </rdf:Description>
                                    </rdf:li>
                                      <rdf:li>
                                        <rdf:Description>
                                            <pdfaProperty:name>object</pdfaProperty:name>
                                            <pdfaProperty:valueType>open Choice</pdfaProperty:valueType>
                                            <pdfaProperty:category>internal</pdfaProperty:category>
                                            <pdfaProperty:description>Object type for which the property is allowed to use (Page, Content, Font, etc.)</pdfaProperty:description>
                                        </rdf:Description>
                                    </rdf:li>
                               </rdf:Seq>
                            </pdfaSchema:property>
                        </rdf:Description>
                    </rdf:li>
                    <!--PDF/A identification schema -->
                    <rdf:li rdf:parseType = "Resource">
                        <pdfaSchema:schema>PDF/A identification schema</pdfaSchema:schema>
                        <pdfaSchema:namespaceURI>http://www.aiim.org/pdfa/ns/id/</pdfaSchema:namespaceURI>
                        <pdfaSchema:prefix>pdfaid</pdfaSchema:prefix>
                        <pdfaSchema:property>
                            <rdf:Seq>
                                <rdf:li>
                                    <rdf:Description>
                                        <pdfaProperty:name>amd</pdfaProperty:name>
                                        <pdfaProperty:valueType>Text</pdfaProperty:valueType>
                                        <pdfaProperty:category>internal</pdfaProperty:category>
                                        <pdfaProperty:description>Optional PDF/A amendment identifier</pdfaProperty:description>
                                    </rdf:Description>
                                </rdf:li>
                                <rdf:li>
                                    <rdf:Description pdfaValidate:status = "required" pdfaValidate:default = "B" pdfaValidate:constraint = "^A|B$" pdfaValidate:standard = "pdfa" pdfaValidate:clause = "6.7.11">
                                        <pdfaProperty:name>conformance</pdfaProperty:name>
                                        <pdfaProperty:valueType>Text</pdfaProperty:valueType>
                                        <pdfaProperty:category>internal</pdfaProperty:category>
                                        <pdfaProperty:description>PDF/A conformance level: A or B</pdfaProperty:description>
                                    </rdf:Description>
                                </rdf:li>
                                <rdf:li>
                                    <rdf:Description pdfaValidate:status = "required" pdfaValidate:default = "1" pdfaValidate:constraint = "^1$" pdfaValidate:standard = "pdfa" pdfaValidate:clause = "6.7.11">
                                        <pdfaProperty:name>part</pdfaProperty:name>
                                        <pdfaProperty:valueType>Integer</pdfaProperty:valueType>
                                        <pdfaProperty:category>internal</pdfaProperty:category>
                                        <pdfaProperty:description>PDF/A version identifier</pdfaProperty:description>
                                    </rdf:Description>
                                </rdf:li>
                            </rdf:Seq>
                        </pdfaSchema:property>
                    </rdf:li>
                    <!--Auxiliary Predefined Schemas  (Value Type or Field schemas)  -->
                    <!--Dimension Value Type schema -->
                    <rdf:li rdf:parseType = "Resource">
                        <pdfaSchema:schema>Dimension Value Type schema</pdfaSchema:schema>
                        <pdfaSchema:namespaceURI>http://ns.adobe.com/xap/1.0/sType/Dimensions#</pdfaSchema:namespaceURI>
                        <pdfaSchema:prefix>stDim</pdfaSchema:prefix>
                        <pdfaSchema:valueType>
                            <rdf:Seq>
                                <rdf:li rdf:parseType = "Resource">
                                    <pdfaType:description>Structure containing dimensions for a drawn object.</pdfaType:description>
                                    <pdfaType:namespaceURI>http://ns.adobe.com/xap/1.0/sType/Dimensions#</pdfaType:namespaceURI>
                                    <pdfaType:prefix>stDim</pdfaType:prefix>
                                    <pdfaType:type>Dimensions</pdfaType:type>
                                    <pdfaType:field>
                                        <rdf:Seq>
                                            <rdf:li>
                                                <rdf:Description pdfaValidate:status = "required" pdfaValidate:constraint = "(0,)" pdfaValidate:standard = "xmp" pdfaValidate:clause = "3.2">
                                                    <pdfaField:name>w</pdfaField:name>
                                                    <pdfaField:valueType>Real</pdfaField:valueType>
                                                    <pdfaField:description>Width</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description pdfaValidate:status = "required" pdfaValidate:constraint = "(0,)" pdfaValidate:standard = "xmp" pdfaValidate:clause = "3.2">
                                                    <pdfaField:name>h</pdfaField:name>
                                                    <pdfaField:valueType>Real</pdfaField:valueType>
                                                    <pdfaField:description>Height</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description pdfaValidate:status = "required" pdfaValidate:default = "pixel"  pdfaValidate:standard = "xmp" pdfaValidate:clause = "3.2">
                                                    <pdfaField:name>unit</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>Units. For example: inch, mm, pixel, pica, point</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                        </rdf:Seq>
                                    </pdfaType:field>
                                </rdf:li>
                            </rdf:Seq>
                        </pdfaSchema:valueType>
                    </rdf:li>
                    <!--Thumbnail Value Type schema -->
                    <rdf:li rdf:parseType = "Resource">
                        <pdfaSchema:schema>Thumbnail Value Type schema</pdfaSchema:schema>
                        <pdfaSchema:namespaceURI>http://ns.adobe.com/xap/1.0/g/img/</pdfaSchema:namespaceURI>
                        <pdfaSchema:prefix>xmpGImg</pdfaSchema:prefix>
                        <pdfaSchema:valueType>
                            <rdf:Seq>
                                <rdf:li rdf:parseType = "Resource">
                                    <pdfaType:description>A thumbnail image for a file.</pdfaType:description>
                                    <pdfaType:namespaceURI>http://ns.adobe.com/xap/1.0/g/img/</pdfaType:namespaceURI>
                                    <pdfaType:prefix>xmpGImg</pdfaType:prefix>
                                    <pdfaType:type>Thumbnail</pdfaType:type>
                                    <pdfaType:field>
                                        <rdf:Seq>
                                            <rdf:li>
                                                <rdf:Description pdfaValidate:status = "required" pdfaValidate:constraint = "^JPEG$" pdfaValidate:standard = "xmp" pdfaValidate:clause = "3.2">
                                                    <pdfaField:name>format</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>The image encoding. Defined value: JPEG.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description pdfaValidate:status = "required" pdfaValidate:constraint = "(0,)" pdfaValidate:standard = "xmp" pdfaValidate:clause = "3.2">
                                                    <pdfaField:name>height</pdfaField:name>
                                                    <pdfaField:valueType>Integer</pdfaField:valueType>
                                                    <pdfaField:description>Height in pixels</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description pdfaValidate:status = "required" pdfaValidate:constraint = "(0,)" pdfaValidate:standard = "xmp" pdfaValidate:clause = "3.2">
                                                    <pdfaField:name>width</pdfaField:name>
                                                    <pdfaField:valueType>Integer</pdfaField:valueType>
                                                    <pdfaField:description>Width in pixels</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description pdfaValidate:status = "required" pdfaValidate:constraint = "base64" pdfaValidate:standard = "xmp" pdfaValidate:clause = "3.2">
                                                    <pdfaField:name>image</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>The full thumbnail image data, converted to base 64 notation (according to section 6.8 of RFC 2045). This is the thumbnail data typically found in a digital image, such as the value of tag 513 in a JPEG stream.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                        </rdf:Seq>
                                    </pdfaType:field>
                                </rdf:li>
                            </rdf:Seq>
                        </pdfaSchema:valueType>
                    </rdf:li>
                    <!--ResourceEvent Value Type schema -->
                    <rdf:li rdf:parseType = "Resource">
                        <pdfaSchema:schema>ResourceEvent Value Type schema</pdfaSchema:schema>
                        <pdfaSchema:namespaceURI>http://ns.adobe.com/xap/1.0/sType/ResourceEvent#</pdfaSchema:namespaceURI>
                        <pdfaSchema:prefix>stEvt</pdfaSchema:prefix>
                        <pdfaSchema:valueType>
                            <rdf:Seq>
                                <rdf:li rdf:parseType = "Resource">
                                    <pdfaType:description>Event that occurred in the processing of this document</pdfaType:description>
                                    <pdfaType:namespaceURI>http://ns.adobe.com/xap/1.0/sType/ResourceEvent#</pdfaType:namespaceURI>
                                    <pdfaType:prefix>stEvt</pdfaType:prefix>
                                    <pdfaType:type>ResourceEvent</pdfaType:type>
                                    <pdfaType:field>
                                        <rdf:Seq>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>action</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>The action that occurred.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>instanceID</pdfaField:name>
                                                    <pdfaField:valueType>URI</pdfaField:valueType>
                                                    <pdfaField:description>The instance ID of the modified resource.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>parameters</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>Additional description of the action.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>softwareAgent</pdfaField:name>
                                                    <pdfaField:valueType>AgentName</pdfaField:valueType>
                                                    <pdfaField:description>The software agent that performed the action.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>when</pdfaField:name>
                                                    <pdfaField:valueType>Date</pdfaField:valueType>
                                                    <pdfaField:description>Optional timestamp of when the action occurred.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                        </rdf:Seq>
                                    </pdfaType:field>
                                </rdf:li>
                            </rdf:Seq>
                        </pdfaSchema:valueType>
                    </rdf:li>
                    <!--ResourceRef Value Type schema -->
                    <rdf:li rdf:parseType = "Resource">
                        <pdfaSchema:schema>ResourceRef Value Type schema</pdfaSchema:schema>
                        <pdfaSchema:namespaceURI>http://ns.adobe.com/xap/1.0/sType/ResourceRef#</pdfaSchema:namespaceURI>
                        <pdfaSchema:prefix>stRef</pdfaSchema:prefix>
                        <pdfaSchema:valueType>
                            <rdf:Seq>
                                <rdf:li rdf:parseType = "Resource">
                                    <pdfaType:description>A multiple part reference to a resource.</pdfaType:description>
                                    <pdfaType:namespaceURI>http://ns.adobe.com/xap/1.0/sType/ResourceRef#</pdfaType:namespaceURI>
                                    <pdfaType:prefix>stRef</pdfaType:prefix>
                                    <pdfaType:type>ResourceRef</pdfaType:type>
                                    <pdfaType:field>
                                        <rdf:Seq>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>instanceID</pdfaField:name>
                                                    <pdfaField:valueType>URI</pdfaField:valueType>
                                                    <pdfaField:description>The referenced resource instance ID.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>documentID</pdfaField:name>
                                                    <pdfaField:valueType>URI</pdfaField:valueType>
                                                    <pdfaField:description>The referenced resource xmpMM:DocumentID.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>versionID</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>The referenced resource xmpMM:VersionID.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>renditionClass</pdfaField:name>
                                                    <pdfaField:valueType>RenditionClass</pdfaField:valueType>
                                                    <pdfaField:description>The referenced resource xmpMM:RenditionClass.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>renditionParams</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>The referenced resource xmpMM:RenditionParams.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>manager</pdfaField:name>
                                                    <pdfaField:valueType>AgentName</pdfaField:valueType>
                                                    <pdfaField:description>The referenced resource xmpMM:Manager.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>managerVariant</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>The referenced resource xmpMM:Manager-Variant.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>manageTo</pdfaField:name>
                                                    <pdfaField:valueType>URI</pdfaField:valueType>
                                                    <pdfaField:description>The referenced resource xmpMM:ManageTo.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>manageUI</pdfaField:name>
                                                    <pdfaField:valueType>URI</pdfaField:valueType>
                                                    <pdfaField:description>The referenced resource xmpMM:ManageUI.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                        </rdf:Seq>
                                    </pdfaType:field>
                                </rdf:li>
                            </rdf:Seq>
                        </pdfaSchema:valueType>
                    </rdf:li>
                    <!--Version Value Type schema -->
                    <rdf:li rdf:parseType = "Resource">
                        <pdfaSchema:schema>Version Value Type schema</pdfaSchema:schema>
                        <pdfaSchema:namespaceURI>http://ns.adobe.com/xap/1.0/sType/Version#</pdfaSchema:namespaceURI>
                        <pdfaSchema:prefix>stVer</pdfaSchema:prefix>
                        <pdfaSchema:valueType>
                            <rdf:Seq>
                                <rdf:li rdf:parseType = "Resource">
                                    <pdfaType:description>Version of a document</pdfaType:description>
                                    <pdfaType:namespaceURI>http://ns.adobe.com/xap/1.0/sType/Version#</pdfaType:namespaceURI>
                                    <pdfaType:prefix>stVer</pdfaType:prefix>
                                    <pdfaType:type>Version</pdfaType:type>
                                    <pdfaType:field>
                                        <rdf:Seq>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>comments</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>Comments concerning what was changed.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>event</pdfaField:name>
                                                    <pdfaField:valueType>ResourceEvent</pdfaField:valueType>
                                                    <pdfaField:description>High level, formal description of what operation the user performed.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>modifier</pdfaField:name>
                                                    <pdfaField:valueType>ProperName</pdfaField:valueType>
                                                    <pdfaField:description>The person who modified this version.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>modifyDate</pdfaField:name>
                                                    <pdfaField:valueType>Date</pdfaField:valueType>
                                                    <pdfaField:description>The date on which this version was checked in.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>version</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>The new version number.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                        </rdf:Seq>
                                    </pdfaType:field>
                                </rdf:li>
                            </rdf:Seq>
                        </pdfaSchema:valueType>
                    </rdf:li>
                    <!--Job Value Type schema -->
                    <rdf:li rdf:parseType = "Resource">
                        <pdfaSchema:schema>Job Value Type schema</pdfaSchema:schema>
                        <pdfaSchema:namespaceURI>http://ns.adobe.com/xap/1.0/sType/Job#</pdfaSchema:namespaceURI>
                        <pdfaSchema:prefix>stJob</pdfaSchema:prefix>
                        <pdfaSchema:valueType>
                            <rdf:Seq>
                                <rdf:li rdf:parseType = "Resource">
                                    <pdfaType:description>Job for a job-management system</pdfaType:description>
                                    <pdfaType:namespaceURI>http://ns.adobe.com/xap/1.0/sType/Job#</pdfaType:namespaceURI>
                                    <pdfaType:prefix>stJob</pdfaType:prefix>
                                    <pdfaType:type>Job</pdfaType:type>
                                    <pdfaType:field>
                                        <rdf:Seq>
                                            <rdf:li>
                                                <rdf:Description pdfaValidate:status = "required" pdfaValidate:standard = "xmp" pdfaValidate:clause = "3.2">
                                                    <pdfaField:name>id</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>Unique ID for the job. This field is a reference into some external job management system.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>name</pdfaField:name>
                                                    <pdfaField:valueType>Text</pdfaField:valueType>
                                                    <pdfaField:description>Informal name of job. This name is for user display and informal systems.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                            <rdf:li>
                                                <rdf:Description>
                                                    <pdfaField:name>url</pdfaField:name>
                                                    <pdfaField:valueType>URL</pdfaField:valueType>
                                                    <pdfaField:description>A file URL referencing an external job management file.</pdfaField:description>
                                                </rdf:Description>
                                            </rdf:li>
                                        </rdf:Seq>
                                    </pdfaType:field>
                                </rdf:li>
                            </rdf:Seq>
                        </pdfaSchema:valueType>
                    </rdf:li>
                    <!--Identifier Value Type schema -->
                    <rdf:li rdf:parseType = "Resource">
                        <pdfaSchema:schema>Identifier Value Type schema</pdfaSchema:schema>
                        <pdfaSchema:namespaceURI>http://ns.adobe.com/xmp/Identifier/qual/1.0/</pdfaSchema:namespaceURI>
                        <pdfaSchema:prefix>xmpidq</pdfaSchema:prefix>
                        <pdfaSchema:valueType>
